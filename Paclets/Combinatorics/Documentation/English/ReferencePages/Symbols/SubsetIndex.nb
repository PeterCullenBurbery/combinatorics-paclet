Cell[TextData[{
 "For any subset, a unique index ",
 Cell[BoxData[
  StyleBox["n", "TI"]], "Text", "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "03379ffe-c8c7-41da-9a0b-2da7088d6b88"],
