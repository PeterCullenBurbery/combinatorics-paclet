  TagBox[
   FormBox[
    RowBox[{
     FormBox[
      StyleBox["n", "TI"],
      TraditionalForm], "=", 
     RowBox[{
      FormBox[
       UnderoverscriptBox["\[Sum]", 
        RowBox[{
         StyleBox["j", "TI"], "=", "1"}], 
        StyleBox["k", "TI"]],
