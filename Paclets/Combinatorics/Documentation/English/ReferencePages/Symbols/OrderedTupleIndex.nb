Cell[TextData[{
 "Ordered tuples are considered to be lists ",
 Cell[BoxData[
  RowBox[{"{", 
   RowBox[{
    SubscriptBox[
     StyleBox["a", "TI"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["a", "TI"], 
     StyleBox["2", "TR"]], ",", 
    StyleBox["\[Ellipsis]", "TR"], ",", 
    SubscriptBox[
     StyleBox["a", "TI"], 
     StyleBox["k", "TI"]]}], "}"}]],
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "c178abe0-25d9-4691-92b8-031feb8906e4"],
 " such that the ",
 Cell[BoxData[
  SubscriptBox[
   StyleBox["a", "TI"], 
   StyleBox["i", "TI"]]], "Text", "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "aa0ac9a1-c06c-4e71-b489-4eda291d2656"],
 " are non-negative and non-decreasing."
}], "Notes",
 TaggingRules->{},
 CellID->96713216,ExpressionUUID->"d2c6d3a4-f84c-41ba-9ae0-166f8d9255a7"],

Cell[TextData[{
 "For any ordered tuple, a unique index ",
 Cell[BoxData[
  StyleBox["n", "TI"]], "Text", "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "c22bdc8d-f722-4ef1-a611-3ff2b32a1b59"],
 " can be defined via ",
 Cell[BoxData[
  TagBox[
   FormBox[
    RowBox[{
     StyleBox["n", "TI"], "=", 
     RowBox[{
      FormBox[
       UnderoverscriptBox["\[Sum]", 
        RowBox[{
         StyleBox["j", "TI"], "=", "0"}], 
        RowBox[{
         StyleBox["k", "TI"], "-", "1"}]],
       TraditionalForm], 
      TemplateBox[{
        FormBox[
         RowBox[{
           SubscriptBox[
            StyleBox["a", "TI"], 
            StyleBox["j", "TI"]], "+", 
           StyleBox["j", "TI"]}], TraditionalForm], 
        FormBox[
         RowBox[{
           StyleBox["j", "TI"], "+", "1"}], TraditionalForm]},
       "Binomial"]}]}],
    TraditionalForm],
   "ForceRasterization"]],ExpressionUUID->
  "a5efe362-6511-40e6-a192-2faff6bd1c50"],
 "."
}], "Notes",
 TaggingRules->{},
 CellID->953021534,ExpressionUUID->"e5c0ddaf-c0c1-442d-99eb-ebc989311b93"],

Cell[TextData[{
 "For example, subset {1, 2, 2} has index ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", GridBox[{
        {
         RowBox[{"1", "+", "0"}]},
        {
         RowBox[{"0", "+", "1"}]}
       }], ")"}], "+", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"2", "+", "1"}]},
        {
         RowBox[{"1", "+", "1"}]}
       }], ")"}], "+", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"2", "+", "2"}]},
        {
         RowBox[{"2", "+", "1"}]}
       }], ")"}], "+", "1"}], "=", "9"}], TraditionalForm]],ExpressionUUID->
  "d927dc78-7490-4159-97f8-7d342302847b"],
 "."
}], "Notes",
 TaggingRules->{},
 CellID->163234066,ExpressionUUID->"ac13233b-6816-493d-b899-75a017b31881"]
}, Open  ]],

