Cell[TextData[{
 "Ordered tuples are considered to be lists ",
 Cell[BoxData[
  RowBox[{"{", 
   RowBox[{
    SubscriptBox[
     StyleBox["a", "TI"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["a", "TI"], 
     StyleBox["2", "TR"]], ",", 
    StyleBox["\[Ellipsis]", "TR"], ",", 
    SubscriptBox[
     StyleBox["a", "TI"], 
     StyleBox["k", "TI"]]}], "}"}]],
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "246967f0-69e0-4a12-81e7-17efdb0dfeb5"],
 " such that the ",
 Cell[BoxData[
  SubscriptBox[
   StyleBox["a", "TI"], 
   StyleBox["i", "TI"]]], "Text", "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "a67db55e-5305-4443-bcee-6f618c28c9d0"],
 " are non-negative and non-decreasing."
}], "Notes",
 CellID->96713216,ExpressionUUID->"06b5bb5b-dbcc-475c-9033-ffe00fc034c1"],

Cell[TextData[{
 "For any ordered tuple, a unique index ",
 Cell[BoxData[
  StyleBox["n", "TI"]], "Text", "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "f8d7c7e5-f1de-47d9-9120-cce4286b1bce"],
 " can be defined via ",
 Cell[BoxData[
  TagBox[
   FormBox[
    RowBox[{
     StyleBox["n", "TI"], "=", 
     RowBox[{
      FormBox[
       UnderoverscriptBox["\[Sum]", 
        RowBox[{
         StyleBox["j", "TI"], "=", "0"}], 
        RowBox[{
         StyleBox["k", "TI"], "-", "1"}]],
       TraditionalForm], 
      TemplateBox[{
        FormBox[
         RowBox[{
           SubscriptBox[
            StyleBox["a", "TI"], 
            StyleBox["j", "TI"]], "+", 
           StyleBox["j", "TI"]}], TraditionalForm], 
        FormBox[
         RowBox[{
           StyleBox["j", "TI"], "+", "1"}], TraditionalForm]},
       "Binomial"]}]}],
    TraditionalForm],
   "ForceRasterization"]],ExpressionUUID->
  "3ab5cf7a-c402-4675-9ac1-19326e1b643e"],
 "."
}], "Notes",
 CellID->953021534,ExpressionUUID->"0e6fc5db-6a0d-47c0-b183-c848faacc544"],

Cell[TextData[{
 "For example, ordered tuple {1, 2, 2} has index ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", GridBox[{
        {
         RowBox[{"1", "+", "0"}]},
        {
         RowBox[{"0", "+", "1"}]}
       }], ")"}], "+", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"2", "+", "1"}]},
        {
         RowBox[{"1", "+", "1"}]}
       }], ")"}], "+", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"2", "+", "2"}]},
        {
         RowBox[{"2", "+", "1"}]}
       }], ")"}], "+", "1"}], "=", "9"}], TraditionalForm]],ExpressionUUID->
  "b0587a20-88f4-4275-bacc-7567de94b231"],
 "."
}], "Notes",
