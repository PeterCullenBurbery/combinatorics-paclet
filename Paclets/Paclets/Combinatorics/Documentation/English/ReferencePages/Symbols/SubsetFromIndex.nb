Cell[TextData[{
 "Subsets are considered to be lists ",
 Cell[BoxData[
  RowBox[{"{", 
   RowBox[{
    SubscriptBox[
     StyleBox["a", "TI"], 
     StyleBox["1", "TR"]], ",", 
    SubscriptBox[
     StyleBox["a", "TI"], 
     StyleBox["2", "TR"]], ",", 
    StyleBox["\[Ellipsis]", "TR"], ",", 
    SubscriptBox[
     StyleBox["a", "TI"], 
     StyleBox["k", "TI"]]}], "}"}]],
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "89402def-73e6-4525-9054-086361e1dd74"],
 " such that the ",
 Cell[BoxData[
  SubscriptBox[
   StyleBox["a", "TI"], 
   StyleBox["i", "TI"]]], "Text", "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "223cedbb-06fa-4420-a8a3-5d34e8a90315"],
 " are non-negative integers and strictly increasing."
}], "Notes",
 TaggingRules->{},
 CellID->789602537,ExpressionUUID->"714cf846-9655-4d12-8069-a3f153dfcfac"],

Cell[TextData[{
 "Subsets are sorted by their reverse; ",
