  RowBox[{"{", 
   RowBox[{"1", ",", "2", ",", "3"}], "}"}]], "Text", "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "3bd5c1a3-80a9-4b79-a4fd-bbd5a9fc4e56"],
